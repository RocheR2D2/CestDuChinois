{% extends 'dashboardbase.html.twig' %}

	{% block content %}
        {{ form_start(form) }}
        {{ form_errors(form) }}

        {{ form_row(form.username) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.sex) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.profil) }}

        {{ form_row(form.register, { 'label': 'Modifier' }) }}
        {{ form_end(form) }}
    {% endblock %}